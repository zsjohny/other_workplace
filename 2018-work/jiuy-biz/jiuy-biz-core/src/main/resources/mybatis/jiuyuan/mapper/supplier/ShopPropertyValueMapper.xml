<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.store.dao.mapper.ShopPropertyValueMapper">


	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.store.entity.ShopPropertyValue">
		<id column="id" property="id" />
		<result column="store_id" property="storeId" />
		<result column="property_value" property="propertyValue" />
		<result column="property_name_id" property="propertyNameId" />
		<result column="order_index" property="orderIndex" />
		<result column="status" property="status" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, store_id AS storeId, property_value AS propertyValue, property_name_id AS propertyNameId, order_index AS orderIndex, status, create_time AS createTime, update_time AS updateTime
    </sql>


</mapper>
