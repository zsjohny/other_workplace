<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.store.dao.mapper.StoreWxaMapper">


    <select id="selectStoreWxa" resultType="com.jiuyuan.entity.store.StoreWxa">
            select `id` from `jiuy_store_wxa` where `store_id`=#{storeId}
    </select>

    <select id="selectByAppId" resultType="com.jiuyuan.entity.store.StoreWxa">
            select `id` from `jiuy_store_wxa` where `app_id`=#{appId}
    </select>
</mapper>
