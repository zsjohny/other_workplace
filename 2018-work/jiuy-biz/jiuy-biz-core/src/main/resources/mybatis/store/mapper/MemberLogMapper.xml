<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiuyuan.dao.mapper.store.MemberLogMapper">


	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.yujj.entity.product.MemberLog">
		<id column="id" property="id" />
		<result column="member_id" property="memberId" />
		<result column="source" property="source" />
		<result column="order_no" property="orderNo" />
		<result column="before_del_state" property="beforeDelState" />
		<result column="after_del_state" property="afterDelState" />
		<result column="before_total_money" property="beforeTotalMoney" />
		<result column="after_total_money" property="afterTotalMoney" />
		<result column="before_end_time" property="beforeEndTime" />
		<result column="after_end_time" property="afterEndTime" />
		<result column="before_time_queue" property="beforeTimeQueue" />
		<result column="after_time_queue" property="afterTimeQueue" />
		<result column="create_time" property="createTime" />
		<result column="create_user_id" property="createUserId" />
		<result column="before_wx_closed_time" property="beforeWxClosedTime" />
		<result column="after_wx_closed_time" property="afterWxClosedTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, member_id AS memberId, source, order_no AS orderNo,
        before_del_state AS beforeDelState, after_del_state AS afterDelState,
        before_total_money AS beforeTotalMoney, after_total_money AS afterTotalMoney,
         before_end_time AS beforeEndTime, after_end_time AS afterEndTime, before_time_queue
         AS beforeTimeQueue, after_time_queue AS afterTimeQueue, create_time AS createTime,
         create_user_id AS createUserId, before_wx_closed_time AS beforeWxClosedTime, after_wx_closed_time AS afterWxClosedTime
    </sql>

</mapper>
