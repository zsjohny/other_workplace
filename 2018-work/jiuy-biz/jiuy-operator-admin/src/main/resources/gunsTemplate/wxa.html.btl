<script>
	$(function(){
		var innerHeight = window.innerHeight || document.documentElement.innerHeight || document.body.innerHeight;
        var header = $(".header-container").height();
        $(".body-container").height((innerHeight-header)+"px");
        $(".body-text-next .icon").on("click",function(){
            $(".body-container .body-text").hide();
            $(".body-container").animate({height:0},500);
            $(".section,.footer,.ui-suspend-box").show();
        });
        $(document).on("mousewheel",function(event, delta){
           var dir = delta > 0 ? 'Up' : 'Down';
            if (dir == 'Up') {
                console.log("向上滚动");
            } else {
                console.log("向下滚动");
                 $(".body-container .body-text").hide();
                 $(".body-container").animate({height:0},500);
                 $(".section,.footer,.ui-suspend-box").show();
            }
        })
	})
</script>
