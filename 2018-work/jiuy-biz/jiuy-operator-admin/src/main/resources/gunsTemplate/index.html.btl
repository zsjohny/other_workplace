    <script>
        var mySwiper = new Swiper('.header-banner', {
            autoplay: true, //可选选项，自动滑动
            width: window.innerWidth,

        })
        $(".section-product-content-two-dd-img02").mouseover(function(){

            $("#section-product-content-two-dd-product1").attr("src","img/product03.png?v1.0")
        }).mouseout(function () {
            $("#section-product-content-two-dd-product1").attr("src","img/product02.png")
//            $(".section-product-content-two-dd-img02").removeClass(" bounceIn")
        });



        $(".section-product-content-two-dd-img01").mouseover(function(){
            $("#section-product-content-two-dd-product2").fadeIn(1000).attr("src","img/product04.png")

        }).mouseout(function () {
            $("#section-product-content-two-dd-product2").fadeIn(1000).attr("src","img/product01.png")
//            $(".section-product-content-two-dd-img01").removeClass(" bounceIn")
        });

        var mySwiper02 = new Swiper ('.section-team-carousel', {

            loop: true,

            // 如果需要分页器
            pagination: {
                el: '.section-team .swiper-pagination',
                clickable: true,
                renderBullet: function (index, className) {
                    return '<span class="' + className + '">' + (index + 1) + '</span>';
                },
            },

            // 如果需要前进后退按钮
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
            autoplay: true, //可选选项，自动滑动


        })

        //窗口显示才加载

        $('.counter').countUp(
            {   delay: 100,
                time: 8000
            }
        );

    

    </script>
