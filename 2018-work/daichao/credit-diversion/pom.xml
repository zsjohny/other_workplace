<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.songxm.credit</groupId>
    <artifactId>credit-diversion</artifactId>
    <version>1.0.0-SNAPSHOT</version>

    <packaging>pom</packaging>

    <name>credit-diversion</name>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>1.5.8.RELEASE</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <core.version>1.4-SNAPSHOT</core.version>
        <hibernate.validator>5.0.1.Final</hibernate.validator>
        <bean-validation.version>1.1.0.Final</bean-validation.version>
        <commons.httpclient.version>4.5.1</commons.httpclient.version>
        <xbcommon.version>1.2-SNAPSHOT</xbcommon.version>
        <pagehelper.version>4.1.4</pagehelper.version>
        <common-logging.version>1.0.0-SNAPSHOT</common-logging.version>
        <mapper.version>2.3.4</mapper.version>
        <mysql.version>5.1.40</mysql.version>
        <pagehelper.version>3.7.5</pagehelper.version>
        <jsqlparser.version>0.9.1</jsqlparser.version>
    </properties>


    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>moxie.cloud</groupId>
                <artifactId>core-server</artifactId>
                <version>${core.version}</version>
            </dependency>
            <dependency>
                <groupId>moxie.cloud</groupId>
                <artifactId>core-client</artifactId>
                <version>${core.version}</version>
            </dependency>
            <dependency>
                <groupId>moxie.cloud</groupId>
                <artifactId>core-common</artifactId>
                <version>${core.version}</version>
            </dependency>
            <dependency>
                <groupId>com.moxie.cloud.common</groupId>
                <artifactId>commons-config</artifactId>
                <version>1.3-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.moxie.commons</groupId>
                <artifactId>commons-utils</artifactId>
                <version>1.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.xinbang.common</groupId>
                <artifactId>xbcommon</artifactId>
                <version>1.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.xinbang.cif</groupId>
                <artifactId>client</artifactId>
                <version>2.0.0-SNAPSHOT</version>
            </dependency>

            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-jdbc</artifactId>
                <version>1.5.3.RELEASE</version>
            </dependency>

            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-aop</artifactId>
                <version>1.5.3.RELEASE</version>
            </dependency>

            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>druid</artifactId>
                <version>1.0.14</version>
            </dependency>
            <dependency>
                <groupId>javax.validation</groupId>
                <artifactId>validation-api</artifactId>
                <version>${bean-validation.version}</version>
            </dependency>
            <dependency>
                <groupId>org.hibernate</groupId>
                <artifactId>hibernate-validator</artifactId>
                <version>${hibernate.validator}</version>
            </dependency>
            <dependency>
                <groupId>com.github.pagehelper</groupId>
                <artifactId>pagehelper</artifactId>
                <version>${pagehelper.version}</version>
            </dependency>
            <dependency>
                <groupId>com.xinbang.common</groupId>
                <artifactId>common-logging</artifactId>
                <version>${common-logging.version}</version>
            </dependency>
            <!-- 通用Mapper -->
            <dependency>
                <groupId>com.github.abel533</groupId>
                <artifactId>mapper</artifactId>
                <version>${mapper.version}</version>
            </dependency>
            <!-- 分页助手 -->
            <dependency>
                <groupId>com.github.pagehelper</groupId>
                <artifactId>pagehelper</artifactId>
                <version>${pagehelper.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.jsqlparser</groupId>
                <artifactId>jsqlparser</artifactId>
                <version>${jsqlparser.version}</version>
            </dependency>
            <dependency>
                <groupId>mysql</groupId>
                <artifactId>mysql-connector-java</artifactId>
                <version>5.1.40</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <encoding>utf-8</encoding>
                </configuration>
            </plugin>
            <plugin>
                <inherited>true</inherited>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
    <distributionManagement>
        <repository>
            <id>xinbang-release</id>
            <name>releases</name>
            <url>http://118.178.238.86:8081/repository/maven-releases</url>
        </repository>
        <snapshotRepository>
            <id>xinbang-snapshots</id>
            <name>snapshots</name>
            <url>http://118.178.238.86:8081/repository/maven-snapshots</url>
        </snapshotRepository>
    </distributionManagement>
    <modules>
        <module>common</module>
        <module>dao</module>
        <module>server</module>
    </modules>
</project>
