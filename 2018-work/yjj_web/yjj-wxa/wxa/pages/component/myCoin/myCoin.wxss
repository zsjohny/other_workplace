/**我的颜值分 **/

/*导航链接列表*/
.mycoin-list{
   border-bottom:16rpx solid #f4f4f4;
}
.mycoin-list .item{
  display: block;
  height: 100rpx;
  position: relative;
  margin: 0 24rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  border-bottom:2rpx solid #f4f4f4;
}
.mycoin-list .item:last-child{
  border-bottom:none;
}
.mycoin-list .item view,
.mycoin-list .item .name{ float: left;}
.mycoin-list .item .icon{
  width: 44rpx;
  height: 44rpx;
  margin-top: 28rpx;
}
.mycoin-list .item .icon1{
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/jiubi-icon.png) no-repeat;
  background-size: 44rpx;
}
.mycoin-list .item .icon2{
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/invite-icon.png) no-repeat;
  background-size:44rpx;
}
.mycoin-list .item .icon3{
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/withdraw-icon.png) no-repeat;
  background-size:44rpx;
}
.mycoin-list .item .name{
  margin-left: 24rpx;
}
.mycoin-list .item .text{
  position: absolute;
  right: 40rpx;
  top: 0;
  color: #B3B3B3;
  font-size: 24rpx;
}
.mycoin-list .item .arrow{
  position: absolute;
  right: -8rpx;
  top: 28rpx;
  width: 44rpx;
  height: 44rpx;
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/arrow-icon.png) no-repeat;
  background-size: 44rpx;
}
/*商品列表*/
.mycoin-box{
  height: auto;
  overflow: hidden;
  font-size: 28rpx;
  box-sizing: border-box;
  background: #fcf9f9;
  margin-bottom: 60rpx;
}
.mycoin-box .title{
  width: 100%;
  height: 100rpx;
  position: relative;
  padding: 0 24rpx;
  font-size: 30rpx;
  line-height: 100rpx;
}
.mycoin-box .title view{float: left;}
.mycoin-box .title .icon{
  width: 44rpx;
  height: 44rpx;
  margin-top: 28rpx;
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/share-icon.png) no-repeat;
  background-size: 44rpx;
}
.mycoin-box .title .name{
  margin-left: 24rpx;
}
.mycoin-box .title .name .text{
  color: #B3B3B3;
  font-size: 24rpx;
}
.product-list{
  width: 702rpx;
  height: auto;
  overflow: hidden;
  padding: 0 24rpx 0 24rpx;
}
.product-list .item{
  width: 345rpx;
  height: auto;
  overflow: hidden;
  margin: 0 12rpx 12rpx 0;
  background: #fff;
  float: left;
  padding-bottom: 16rpx;
}
.product-list .item:nth-child(2n){
  margin-right: 0;
}
.product-list .item .product-img{
  width: 345rpx;
  height: 345rpx;
}
.product-list .item .gray{
  color: #cbc9ca;
}
.product-list .item .text-margin{
  margin-left: 12rpx;
}
.product-list .item .product-title{
  width: 330rpx;
  height: 34rpx;
  line-height: 34rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #858585;
}
.product-list .item .product-price{
  font-size: 30rpx;
  margin: 15rpx 10rpx 0;
}
.product-list .item .product-price .ft-red{
  font-size: 24rpx;
  margin-left: 6rpx;
}
.product-list .item .sales-number{
  font-size: 24rpx;
  margin-top: 8rpx;
}
.product-list .item .share-btn{
  margin: 0 12rpx;
  width: 320rpx;
  height: 52rpx;
  border-radius: 8rpx;
  background: -webkit-linear-gradient(left, #fe3c56 40%, #f1437c);
  background: linear-gradient(left, #fe3c56 40%, #f1437c); 
  font-size: 26rpx;
  text-align: center;
  line-height: 52rpx;
  margin-top: 18rpx;
  color: #fff;
}
.product-list .item .share-btn::after{
  border: none;
}
/**回到顶部**/
.go-top2{
  bottom:270rpx; 
}