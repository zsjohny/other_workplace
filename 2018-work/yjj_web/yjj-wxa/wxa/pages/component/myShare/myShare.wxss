/**我的邀请 **/
.body-contain{
  padding-bottom: 50rpx;
}
.myshare-head .coin-number{
  padding-top: 30rpx;
}
.myshare-head .coin-money{
  text-align: left;
  width: 690rpx;
  margin: 16rpx auto 0;
}
/*邀请列表*/
.myshare-list{
  width: 702rpx;
  margin: 0 auto;
  font-size: 28rpx;
  padding-bottom: 10rpx;
}
.table-head{
  height: 98rpx;
  border-bottom: 2rpx solid #f4f4f4;
  line-height: 98rpx;
  display: -webkit-flex;
  display: flex;
  text-align: center;
}
.table-tr view,
.table-head view{
  width: 33.33%;
  height: 98rpx;
}
.table-head .table-th2{
  position: relative;
}
.table-head .table-th2::before,
.table-head .table-th2::after{
  content: '';
  position: absolute;
  top: 35rpx;
  width: 2rpx;
  height: 28rpx;
  background: #e5e5e5;
  overflow: hidden;
}
.table-head .table-th2::before{
  left: 0;
}
.table-head .table-th2::after{
  right: 0;
}
.table-tr{
  height: 86rpx;
  border-bottom: 2rpx solid #f4f4f4;
  text-align: center;
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.7);
  display: -webkit-flex;
  display: flex;
}
.table-tr view{
  height: 86rpx;
  line-height: 86rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.table-tr .share-time{
  color: rgba(0, 0, 0, 0.6);
  font-size: 22rpx;
}