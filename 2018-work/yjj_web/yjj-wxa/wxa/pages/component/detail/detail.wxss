/**商品详情**/
.tab-box{
  height: auto;
  overflow: hidden;
}
.tab-box .img{
  display: block;
}
.introduce-mod swiper{
  height: 750rpx;
}
.introduce-mod .img{
  width: 750rpx;
  height: 750rpx;
  margin: 0 auto;
}
.swiper-box swiper-item{
  position: relative;
}
.swiper-box swiper-item .video-play{
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 1000;
  margin: -38rpx -73rpx;
  width: 146rpx;
  height: 76rpx;
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/play-btn.png) no-repeat;
