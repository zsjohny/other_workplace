  color: #fff;
  font-size: 20rpx;
}
.activity-mod .activity-info{
  float: left;
  margin-left: 24rpx;
  display: -webkit-flex;
  display: flex;
}
.activity-mod .activity-info .activity-tips{
  display: inline-block;
}
.activity-mod .activity-info .seckill-tips{
  width: 60rpx;
  height: 66rpx;
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/seckell-name.png) no-repeat;
  background-size: 60rpx;
  margin-top: 10rpx;
}
.activity-mod .activity-info .group-tips{
  width: 85rpx;
  height: 41rpx;
  background: url(http://mini-img-main.oss-cn-hangzhou.aliyuncs.com/icon/3.7.7/group-name.png) no-repeat;
  background-size: 85rpx;
  margin-top: 22rpx;
}
.activity-mod .activity-text{
  display: inline-block;
  font-size: 26rpx;
  margin-left: 26rpx;
  padding-top: 26rpx;
}
.activity-mod .time-box{
  width: 260rpx;
  float: right;
}
.activity-mod .time-box .activity-state{
  font-size: 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-left:30rpx; 
}
.activity-mod .time-box .down-time{
  font-size: 20rpx;
  height: 44rpx;
  line-height: 40rpx;
  color: #333;
}
.activity-mod .time-box .down-time text{
  height: 32rpx;
  background: #333;
  display: inline-block;
  line-height: 32rpx;
  text-align: center;
  padding: 0 4rpx;
  border-radius: 6rpx;
  color: #fff;
  margin-right: 10rpx;
  min-width: 38rpx;
}
/**商品标题信息模块**/
.introduce-mod .info-box{
   font-size: 32rpx;
   height: auto;
   overflow: hidden;
   margin: 24rpx 24rpx 0;
   position: relative;
   border-bottom: 2rpx solid #f4f4f4;
   padding-bottom: 24rpx;
}
.introduce-mod .title{
  height: auto;
  overflow: hidden;
}
.introduce-mod .title .p-name{
  display: inline-block;
  max-width:600rpx;
  max-height: 84rpx;
  line-height: 44rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
/*分享按钮模块*/
.share-model{
  position: absolute;
  top: 0;
  right: 0;
  padding-left: 30rpx;
  height: 60rpx;
  text-align: center;
}
.share-model::before{
  width: 2rpx;
  height: 54rpx;
  position: absolute;
  top: 4rpx;
  left: 0;
  content: '';
  background: #f4f4f4;
}
.share-model .icon{
  width: 30rpx;
  height: 30rpx;
  background: url(https://yjj-img-www.oss-cn-hangzhou.aliyuncs.com/15308685400251530868540025.png);
  background-size: 30rpx 30rpx;
  padding: 0;
  margin: 0;
  border: none
}
.share-model .icon::after{
  border: none;
  content:" ";
  width:0%;
  height:0%;
  position:inherit;
  -webkit-transform:scale(0.5);
  transform:scale(0.5);
  border-radius:0;
}
.share-model .ft-text{
  color: #9a9a9a;
  font-size: 20rpx;
  padding-top: 10rpx;
  margin-left: -6rpx;
}
/*无分享功能时提示*/
.share-prompt{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99999;
  background: rgba(0, 0, 0, 0.55);
}
.share-prompt .share-box{
  width: 100%;
  height: 100%;
  position: relative;
}
.share-prompt image{
  width: 108rpx;
  height: 70rpx;
  position: absolute;
  top: 35rpx;
  right: 60rpx;
  z-index: 100000
}
.share-prompt .text{
  text-align: center;
  font-size: 34rpx;
  padding-top: 115rpx;
  color: #fff;
}

/*商品价格模块*/
.stock-tag{
  float: left;
  width: 56rpx;
  height: 30rpx;
  background: #fcefee;
  color: #fd4374;
  font-size: 20rpx;
  font-style: normal;
  line-height: 30rpx;
  text-align: center;
  border-radius: 8rpx;
  margin: 15rpx 0 0 10rpx;
}
.introduce-mod .price-box{
  margin: 20rpx 24rpx 0 0;
  height: auto;
  overflow: hidden;
}
.introduce-mod .price-info{
  float: left;
}
.introduce-mod .new-price{
  color: #ff2742;
  font-size: 24rpx;
  float: left;
}
.introduce-mod .new-price .gray{
  font-size: 24rpx;
  color: #9a9a9a;
  margin-right: 10rpx;
}
.introduce-mod .new-price .number{
  font-size: 44rpx;
}
.introduce-mod .old-price{
  font-size: 24rpx;
  margin-left: 30rpx;
  float: left;
  color: #bfbfbf;
  padding-top: 20rpx;
}
.introduce-mod .old-price .del{
  position: relative;
  margin-left: 10rpx
}
.introduce-mod .old-price .del::after{
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  height: 2rpx;
