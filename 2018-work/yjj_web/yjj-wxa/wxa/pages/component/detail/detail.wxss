/**橱窗视频**/
.slide-video-box {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10000;
}
.slide-video-box .bg{
  width: 100%;
  height: 100%;
  background: #000;
}
.slide-video-box video{
  width: 100%;
